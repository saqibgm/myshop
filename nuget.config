<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
  <!-- If Package Source Mapping is causing issues, disable it temporarily -->
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" /> <!-- Allow all packages from nuget.org -->
    </packageSource>
  </packageSourceMapping>
</configuration>